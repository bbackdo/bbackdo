                <data
                    android:host="oauth"
                    android:scheme="kakaof34797735d54a48073a66f9a9c5ab107" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.kakao.sdk.AppKey"
            android:value="f34797735d54a48073a66f9a9c5ab107" />
    </application>



</manifest>